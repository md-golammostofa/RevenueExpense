  <Code>Public Function ConvertEnNumToBnNum(number As String) As String
        Dim checkval As Decimal = 1.0
        Dim returnVal As String = ""
        Try
            Dim val() As Char = number.ToCharArray()
            For Each item As Char In val
                returnVal += BanglaNumerical(item.ToString())
            Next
        Catch ex As Exception

        End Try

        Return returnVal
End Function

 Public Function BanglaNumerical(enNumerical As String) As String
        Dim val As String = ""

        Select Case enNumerical

            Case = "1"
                val = "১"
            Case = "2"
                val = "২"
            Case = "3"
                val = "৩"
            Case = "4"
                val = "৪"
            Case = "5"
                val = "৫"
            Case = "6"
                val = "৬"
            Case = "7"
                val = "৭"
            Case = "8"
                val = "৮"
            Case = "9"
                val = "৯"
            Case = "0"
                val = "০"
            Case = "."
                val = "."
        End Select

        Return val
    End Function</Code>
